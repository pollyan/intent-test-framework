{
  "name": "变量解析修复测试",
  "description": "测试 aiString 提取用户名并在 ai_input 中使用 ${username} 变量",
  "steps": [
    {
      "action": "navigate",
      "params": {
        "url": "https://example.com"
      },
      "description": "导航到测试页面"
    },
    {
      "action": "aiString",
      "params": {
        "query": "用户名"
      },
      "output_variable": "username",
      "description": "提取用户名到变量"
    },
    {
      "action": "ai_input",
      "params": {
        "text": "${username}",
        "locate": "search box"
      },
      "description": "使用提取的用户名进行输入"
    }
  ]
}